{
  "name": "ReactShare",
  "version": "1.0.0",
  "description": "A peer coding app built in React.js and Express.js",
  "main": "index.js",
  "repository": "https://github.com/flema1/ReactMusicPlayer.git",
  "author": "Franklin Lema",
  "license": "MIT",
  "engines": {
    "node": "8.1.2"
  },
  "scripts": {
    "heroku-postbuild": "yarn build",
    "build": "concurrently \"cd client && yarn build\" \"cd server && yarn build\"",
    "clean": "concurrently \"rimraf node_modules\" \"cd client && rimraf node_modules build\" \"cd server && rimraf node_modules build\"",
    "install": "(cd client && yarn) && (cd server && yarn)",
    "start": "concurrently \"cd client && PORT=3000 yarn start\" \"cd server && PORT=3001 yarn start\"",
    "start:prod": "cd server && yarn start:prod"
  },
  "dependencies": {
    "all": "0.0.0",
    "axios": "^0.16.2",
    "body-parser": "^1.18.1",
    "co-bcryptjs": "^0.2.0",
    "concurrently": "^3.5.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.0.1",
    "method-override": "^2.3.9",
    "morgan": "^1.8.2",
    "node-dir": "^0.1.17",
    "nodemon": "^1.11.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg-promise": "^6.5.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "socket.io-proxy": "^1.0.3"
  }
}
